{
    "compilerOptions": {
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks" : false,
        "noUnusedLocals": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "ESNext",
        "moduleResolution": "node",
        "allowJs": true,
        "noEmit": true,
        "strict": true,
        //"isolatedModules": true
    },
    "types": [
        "jquery"
    ],
    "include": [
      "src"
    ],
    "exclude": [
        "node_modules"
    ]
}